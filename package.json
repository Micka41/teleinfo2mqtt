{
  "name": "teleinfo-mqtt",
  "version": "1.0.0",
  "description": "Publish teleinfo to mqtt topics",
  "main": "app/index.js",
  "scripts": {
    "postinstall": "grep -rl '#!/usr/bin/env node' node_modules/async-mqtt/node_modules/mqtt/ | xargs sed -i 's|#!/usr/bin/env node|// Shebang removed|g'",
    "start": "node app/index",
    "lint": "eslint '**/*.js'",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fmartinou/teleinfo-mqtt.git"
  },
  "keywords": [
    "teleinfo",
    "mqtt"
  ],
  "author": "fmartinou",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fmartinou/teleinfo-mqtt/issues"
  },
  "homepage": "https://github.com/fmartinou/teleinfo-mqtt#readme",
  "dependencies": {
    "@serialport/parser-readline": "9.0.0",
    "async-mqtt": "^2.5.0",
    "bunyan": "1.8.12",
    "deep-equal": "^2.0.3",
    "serialport": "9.0.0"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.1",
    "jest": "^26.0.1",
    "rewire": "^5.0.0"
  }
}
